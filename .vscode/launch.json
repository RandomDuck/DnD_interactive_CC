{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch Program",
      "program": "${workspaceFolder}/node_modules/react-scripts/bin/react-scripts.js",
      "args": ["dev"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch Lint",
      "program": "${workspaceFolder}/node_modules/react-scripts/bin/react-scripts.js",
      "args": ["lint"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Launch Test",
      "program": "${workspaceFolder}/node_modules/react-scripts/bin/react-scripts.js",
      "args": ["test"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Run build",
      "program": "${workspaceFolder}/node_modules/react-scripts/bin/react-scripts.js",
      "args": ["start"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Build program",
      "program": "${workspaceFolder}/node_modules/react-scripts/bin/react-scripts.js",
      "args": ["build"]
    }
  ]
}
